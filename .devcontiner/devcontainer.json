{
    "name": "PostgreSQL Development",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspace",
    "forwardPorts": [5432],
    "postCreateCommand": "psql --version",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-vscode-remote.remote-containers",
          "mtxr.sqltools",
          "mtxr.sqltools-driver-pg"
        ]
      }
    }
  }